{
  "expo": {
    "name": "Smart Farm",
    "slug": "nChuynngnh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/smart-farm.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/smart-farm.png"
      },
      "package": "com.ngonhatthienn.nChuynngnh"
    },
    "runtimeversion": {
      "policy": "sdkVersion"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "5f22943b-f383-4b73-a714-b8ae8dd8d6fb"
      }
    }
  }
}
